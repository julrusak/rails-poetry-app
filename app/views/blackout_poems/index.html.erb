<% @blackout_poems.each do |b| %>
	<%= b.title%><br />
	<%= b.text%><br />
	by <%= link_to b.user.username, user_path(b.user.id) %>
	<% if b.user_id == current_user.id %>
		<%= link_to 'Destroy',  blackout_poem_path(b),  method: :delete, data: { confirm: 'Are you sure?' } %>
	<% end %><br /><br />
<% end %>